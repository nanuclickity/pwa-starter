{
  "name": "react-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "analyze": "npm-run-all clean profile && webpack-bundle-analyzer -h 0.0.0.0 stats.json build/public -s gzip",
    "build-client": "webpack --config config/webpack/client.config.js",
    "build-prod": "NODE_ENV=production npm-run-all build gzip-size",
    "build-server": "webpack --config config/webpack/server.config.js",
    "build": "npm-run-all clean copy build-client build-server",
    "clean": "rm -rf ./build && mkdir -p build/views build/public",
    "copy-public": "cp -r ./src/public ./build/",
    "copy-views": "cp -r ./src/server/views ./build/",
    "copy": "npm-run-all copy-views copy-public",
    "dev-server": "nodemon build/server.bundle.js",
    "generate-pages": "node tools/scripts/generate-pages",
    "gzip-size": "node tools/scripts/gzip-size",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm run build-prod",
    "profile": "NODE_ENV=production webpack --config config/webpack/client.config.js --display normal --profile --json > stats.json",
    "server": "NODE_ENV=production node -r source-map-support/register build/server.bundle.js",
    "start": "npm run server",
    "watch-client": "webpack serve --hot --config config/webpack/client.config.js",
    "watch-server": "webpack --watch --config config/webpack/server.config.js",
    "watch": "npm-run-all clean build-client --parallel watch-client watch-server"
  },
  "private": true,
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@hot-loader/react-dom": "^17.0.1",
    "@popperjs/core": "^2.9.2",
    "apisauce": "^2.1.1",
    "autoprefixer": "10.2.6",
    "axis": "^1.0.0",
    "babel-loader": "8.2.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.2",
    "chalk": "4.1.1",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.1",
    "cookie-parser": "^1.4.5",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano": "^5.0.6",
    "dotenv": "^10.0.0",
    "easy-table": "^1.1.1",
    "eslint-loader": "^4.0.2",
    "express": "4.17.3",
    "file-loader": "6.2.0",
    "fs-extra": "^10.0.0",
    "gzip-size-cli": "^5.0.0",
    "history": "^5.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.0.0",
    "morgan": "^1.10.0",
    "node-sass": "^6.0.1",
    "nodemon": "2.0.9",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss": "8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "6.1.1",
    "pretty-error": "^3.0.4",
    "prop-types": "15.7.2",
    "pug": "3.0.2",
    "purgecss": "^4.0.3",
    "react": "17.0.2",
    "react-dev-utils": "11.0.4",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^12.1.0",
    "source-map-support": "0.5.19",
    "style-loader": "3.0.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "5.42.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-node-externals": "3.0.0",
    "workbox-webpack-plugin": "6.1.5"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "webpack-dev-server": "^3.11.2"
  }
}
