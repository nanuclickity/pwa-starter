{
  "name": "react-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "analyze": "npm-run-all clean profile && webpack-bundle-analyzer -h 0.0.0.0 stats.json build/public -s gzip",
    "build-client": "webpack --config config/webpack/client.config.js",
    "build-prod": "NODE_ENV=production npm-run-all build gzip-size",
    "build-server": "webpack --config config/webpack/server.config.js",
    "build": "npm-run-all clean copy build-client build-server",
    "clean": "rm -rf ./build && mkdir -p build/views build/public",
    "copy-public": "cp -r ./src/public ./build/",
    "copy-views": "cp -r ./src/server/views ./build/",
    "copy-watch-public": "cpx \"./src/public/**/*.*\" ./build/public/ --watch",
    "copy-watch-views": "cpx ./src/server/views/**/*.* ./build/views/ --watch",
    "copy-watch": "npm-run-all --parallel copy-watch-views copy-watch-public",
    "copy": "npm-run-all copy-views copy-public",
    "dev-server": "nodemon build/server.bundle.js",
    "generate-pages": "node tools/scripts/generate-pages",
    "gzip-size": "node tools/scripts/gzip-size",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "npm run build-prod",
    "profile": "NODE_ENV=production webpack --config config/webpack/client.config.js --display normal --profile --json > stats.json",
    "server": "NODE_ENV=production node -r source-map-support/register build/server.bundle.js",
    "start": "npm run server",
    "watch-client": "webpack-dev-server --hot --config config/webpack/client.config.js",
    "watch-server": "webpack --watch --config config/webpack/server.config.js",
    "watch": "npm-run-all clean build-client --parallel copy-watch watch-client watch-server"
  },
  "private": true,
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.9.0",
    "apisauce": "^1.1.0",
    "autoprefixer": "9.6.1",
    "axis": "^1.0.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "chalk": "2.4.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connected-react-router": "^6.5.2",
    "cookie-parser": "^1.4.4",
    "cpx": "^1.5.0",
    "css-loader": "3.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.1.0",
    "easy-table": "^1.1.1",
    "eslint-loader": "^3.0.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "fs-extra": "^8.1.0",
    "gzip-size-cli": "^3.0.0",
    "history": "^4.9.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "nodemon": "1.19.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "1.15.0",
    "postcss": "7.0.17",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "3.0.0",
    "pretty-error": "^2.1.1",
    "prop-types": "15.7.2",
    "pug": "2.0.4",
    "purify-css": "^1.2.5",
    "react": "16.9.0",
    "react-dev-utils": "9.0.3",
    "react-dom": "16.9.0",
    "react-hot-loader": "4.12.12",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.0",
    "source-map-support": "0.5.13",
    "style-loader": "1.0.0",
    "url-loader": "^2.1.0",
    "uuid": "^3.3.3",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0"
  }
}
