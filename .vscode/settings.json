{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "editor.tabSize": 2,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "npm-intellisense.importES6": true,
  "search.exclude": {
    "**/node_modules/*/**": true
  },
  "files.exclude": {
    "**/node_modules/*/**": true,
    "build/": true,
    "dist/": true,
    "package-lock.json": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  }
}
